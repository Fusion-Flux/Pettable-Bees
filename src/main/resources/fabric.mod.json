{
  "schemaVersion": 1,
  "id": "pettablebees",
  "version": "${version}",

  "name": "Pettable Bees",
  "description": "A Minecraft mod that lets you pet bees",
  "authors": [
    "Fusion Flux",
	"slimakiswinka"
  ],
  "contact": {
    "sources": "https://github.com/slimakiswinka/Pettable-Bees",
    "issues": "https://github.com/slimakiswinka/Pettable-Bees/issues"
  },

  "license": "LGPL-3.0",
  "icon": "assets/pettablebees/icon.png",

  "environment": "*",
  "entrypoints": {
